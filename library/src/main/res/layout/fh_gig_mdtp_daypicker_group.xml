<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
  <ImageButton
      android:id="@+id/fh_gig_mdtp_previous_month_arrow"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="@drawable/fh_gig_mdtp_month_arrow_background"
      android:contentDescription="@string/fh_gig_mdtp_previous_month_arrow_description"
      android:minHeight="48dp"
      android:minWidth="48dp"
      android:nextFocusForward="@+id/fh_gig_mdtp_next_month_arrow"
      android:src="@drawable/fh_gig_mdtp_ic_chevron_left_black_24dp"
      android:visibility="invisible"
      />
  <ImageButton
      android:id="@+id/fh_gig_mdtp_next_month_arrow"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:background="@drawable/fh_gig_mdtp_month_arrow_background"
      android:contentDescription="@string/fh_gig_mdtp_next_month_arrow_description"
      android:minHeight="48dp"
      android:minWidth="48dp"
      android:src="@drawable/fh_gig_mdtp_ic_chevron_right_black_24dp"
      android:visibility="invisible"
      />
</FrameLayout>